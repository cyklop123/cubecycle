<% for(var i=1; i<=7; i++){ %>
<div class="col-12 col-sm-6 col-md-4 col-lg-3">
    <div class="card mt-2">
        <div class="card-header">
            Room name <%= i %>
            </div>
            <img class="card-img-top w-50 p-1 m-auto" src="img/3x3.png" alt="Card image cap">
            <ul class="list-group list-group-flush">
            <li class="list-group-item">Type: 3x3</li>
            <li class="list-group-item">Timeout: 30s</li>
            <li class="list-group-item">Admin: jhon1</li>
            </ul>
            <div class="card-body">
            <a href="room" class="btn btn-primary btn-block">Join</a>
            </div>
            <div class="card-footer text-muted">
            <%= (Math.random()>0.5) ? 'Private' : 'Public' %>
            </div>
    </div>
</div>
<% } %>